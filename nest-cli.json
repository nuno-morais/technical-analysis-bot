{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "technical-analysis-bot",
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "web": {
      "type": "application",
      "root": "apps/web",
      "entryFile": "main",
      "sourceRoot": "apps/web/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web/tsconfig.app.json"
      }
    },
    "scheduler": {
      "type": "application",
      "root": "apps/scheduler",
      "entryFile": "main",
      "sourceRoot": "apps/scheduler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scheduler/tsconfig.app.json"
      }
    },
    "analyser": {
      "type": "application",
      "root": "apps/analyser",
      "entryFile": "main",
      "sourceRoot": "apps/analyser/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analyser/tsconfig.app.json"
      }
    }
  }
}