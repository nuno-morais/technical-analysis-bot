{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "technical-analysis-bot",
  "compilerOptions": {
    "webpack": true
  },
  "projects": {
    "scheduler": {
      "type": "application",
      "root": "apps/scheduler",
      "entryFile": "main",
      "sourceRoot": "apps/scheduler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scheduler/tsconfig.app.json"
      }
    },
    "analyser": {
      "type": "application",
      "root": "apps/analyser",
      "entryFile": "main",
      "sourceRoot": "apps/analyser/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analyser/tsconfig.app.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "authentication": {
      "type": "library",
      "root": "libs/authentication",
      "entryFile": "index",
      "sourceRoot": "libs/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authentication/tsconfig.lib.json"
      }
    },
    "symbols.api": {
      "type": "application",
      "root": "apps/symbols.api",
      "entryFile": "main",
      "sourceRoot": "apps/symbols.api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/symbols.api/tsconfig.app.json"
      }
    },
    "notifications.api": {
      "type": "application",
      "root": "apps/notifications.api",
      "entryFile": "main",
      "sourceRoot": "apps/notifications.api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications.api/tsconfig.app.json"
      }
    },
    "symbols": {
      "type": "library",
      "root": "libs/symbols",
      "entryFile": "index",
      "sourceRoot": "libs/symbols/src",
      "compilerOptions": {
        "tsConfigPath": "libs/symbols/tsconfig.lib.json"
      }
    }
  }
}